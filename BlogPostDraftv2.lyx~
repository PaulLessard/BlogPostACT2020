\begin_layout Plain Layout
 
\begin_inset Formula 
\[
\begin{tikzcd}[ampersandreplacement=\&,rowsep=small]\mathsf{ev}:(D^{\star},D)\longrightarrow()\arrow[d,mapsto]\end{tikzcd}
\]

\end_inset


\end_layout

\end_inset


\begin_inset Box Frameless
